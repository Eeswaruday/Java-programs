class Animal 
{
    void make_sound() 
    {
        System.out.println("Animal makes a sound");
    }
}

class Dog extends Animal 
{
    @Override
    void make_sound() 
    {
        System.out.println("Dog barks");
    }
}

class Cat extends Animal {
    @Override
    void make_sound() 
    {
        System.out.println("Cat meows");
    }
}

public class Main {
    public static void main(String args[]) 
    {
        Animal a;
        a = new Dog();
        a.make_sound();
        a = new Cat();
        a.make_sound();
    }
}
